.statictics {
	list-style: none;
	margin: 0 -5px;
	padding: 0;
	overflow: hidden;
}
.statictics li {
	text-align: center;
	float: left;
	margin: 0;
	padding: 0;
	font-size: smaller;
}
.statictics li a { text-decoration: none; display: block; color: #666; padding: 0 5px }
.statictics li a:hover { color: #002C92 }
.statictics li.zero a { color: #999 }
.statictics li strong {
	font-size: 1.5em;
	display: block;
}
.statictics.home li { width: 60px }
.statictics.home li.ad { width: 230px }
