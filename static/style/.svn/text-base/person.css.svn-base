.profile ul.abilities > li.shui[class~="lv5"]   { background-position: -256px 0px}






.profile ul.abilities > li > strong {
	display: block;
	font-size: 12px;
	line-height: 1.5;
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	text-indent: 0;
	background: white;
	text-align: center;
	padding-bottom: 2px;
}
.profile ul.abilities > li.lv1 > strong { color: #036 }
.profile ul.abilities > li.lv2 > strong { color: #60a532 }
.profile ul.abilities > li.lv3 > strong { color: #c39832 }
.profile ul.abilities > li.lv4 > strong { color: #f33298 }
.profile ul.abilities > li.lv5 > strong { color: #002C92 }

.profile dl.meta dt { font-weight: bold; margin-top: 0.4em }
.profile dl.meta dd { margin: 0; padding: 0 }
.profile dl.meta dd.chief a { font-weight: bold }
