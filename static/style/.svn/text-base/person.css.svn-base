.profile ul.abilities > li.litera[class~="lv5"] { background-position: -32px 0px}
.profile ul.abilities > li.layout[class~="lv5"] { background-position: -64px 0px}
.profile ul.abilities > li.web[class~="lv5"]    { background-position: -96px 0px}
.profile ul.abilities > li.music[class~="lv5"]  { background-position: -128px 0px}
.profile ul.abilities > li.sing[class~="lv5"]   { background-position: -160px 0px}
.profile ul.abilities > li.prog[class~="lv5"]   { background-position: -192px 0px}
