<ol class="pager" id="list_pager">
	{% if relations.has_previous %}
	<li id="pre_page_relations" url="?page={{ relations.previous_page_number }}"><a href="#">«前一页</a></li>
	{% else %}
	<li class="prev disabled">«前一页</li>
	{% endif %}
	{% if relations.has_previous %}
	<li class="pages" url="?page={{ relations.previous_page_number }}"><a href="#">{{ relations.previous_page_number }}</a></li>
	{% endif %}
	<li class="current"><a href="#">{{ relations.number }}</a></li>
	{% if relations.next_page_number <= relations.paginator.num_pages %}
	<li class="pages" url="?page={{ relations.next_page_number }}"><a href="#">{{ relations.next_page_number }}</a></li>
	{% endif %}
	{% if relations.next_page_number|add:1 <= relations.paginator.num_pages %}
	<li class="pages" url="?page={{ relations.next_page_number|add:1 }}"><a href="#">{{ relations.next_page_number|add:1 }}</a></li>
	{% endif %}
	<li class="ellipsis">...</li>
	<li class="pages" url="?page={{ relations.paginator.num_pages }}"><a href="#">{{ relations.paginator.num_pages }}</a></li>
	{% if relations.has_next %}
	<li class="next" id="next_page_relations"  url="?page={{ relations.next_page_number }}"><a href="#">后一页»</a></li>
	{% else %}
	<li class="prev disabled">后一页»</li>
	{% endif %}
</ol>

