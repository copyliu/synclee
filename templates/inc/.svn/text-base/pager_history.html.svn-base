<ol class="pager">
	{% if history.has_previous %}
	<li id="pre_page_history" url="?page={{ history.previous_page_number }}"><a href="#">«前一页</a></li>
	{% else %}
	<li class="prev disabled">«前一页</li>
	{% endif %}
	{% if history.has_previous %}
	<li class="pages" url="?page={{ history.previous_page_number }}"><a href="#">{{ history.previous_page_number }}</a></li>
	{% endif %}
	<li class="current"><a href="#">{{ history.number }}</a></li>
	{% if history.next_page_number <= history.paginator.num_pages %}
	<li class="pages" url="?page={{ history.next_page_number }}"><a href="#">{{ history.next_page_number }}</a></li>
	{% endif %}
	{% if history.next_page_number|add:1 <= history.paginator.num_pages %}
	<li class="pages" url="?page={{ history.next_page_number|add:1 }}"><a href="#">{{ history.next_page_number|add:1 }}</a></li>
	{% endif %}
	<li class="ellipsis">...</li>
	<li class="pages" url="?page={{ history.paginator.num_pages }}"><a href="#">{{ history.paginator.num_pages }}</a></li>
	{% if history.has_next %}
	<li class="next" id="next_page_history"  url="?page={{ history.next_page_number }}"><a href="#">后一页»</a></li>
	{% else %}
	<li class="prev disabled">后一页»</li>
	{% endif %}
</ol>