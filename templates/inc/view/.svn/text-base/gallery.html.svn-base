{% for s in chapter.section.all %}
<div class="imagebox" img_title="{{s.title}}" content="{{s.content}}" title="{{s.content}}">
	<img src="/uploads/{{s.picture.url}}" />
	<h2>{{s.title}}</h2>
	{% if s.content %}
	<div class="description" style="display: none" >
		{{s.content}}
	</div>
	{% endif %}
</div>
{% endfor %}
