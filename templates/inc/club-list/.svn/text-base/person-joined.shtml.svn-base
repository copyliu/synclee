{% load thumbnail %}
<div class="projects joined">
	<h3>社团成员 ({{club.members.count}})</h3>
	<ul class="list recommended">
		{% for w in club.get_members %}
		<li><a href="/{{w.username}}/"><img src="/uploads/{% thumbnail w.get_profile.avatar 200x200 %}"><span class="caption">{{w.get_profile.get_name}}</span></a></li>
		{% endfor %}
	</ul>
	<a class="more" href="/search/?query=&author={{the_user.username}}">全部...</a
</div>
