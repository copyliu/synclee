{% extends "base.shtml" %}
{% block title %}查看提醒{% endblock %}

{% block extra_include %}
	{% include "style.html" %}
	{% include "style-project.html" %}
	{% include "style-notification.html" %}
{% endblock %}


{% block content %}
<body class="p-notifications si">{% include "inc/navi.html" %}
  <div class="master">
        <div id="dm" class="notification-type timeline">
            <h3>查看提醒</h3>
            <ol class="list">
                <li class="dm">
                    <cite><a href="/{{from_user.username}}/"><span class="image"><img class="avatar" src="/uploads/{{from_user.get_profile.avatar}}"/></span>{{from_user.get_profile.get_name}}</a></cite>
                    <span class="meta">{{notice.added|timesince}}前</span>
                    <blockquote>{% block notice_content %}{% endblock %}</blockquote>
                </li>
            </ol>
        </div>
</div>
{% include "inc/footer.html" %}
</body>
{% endblock %}